class Solution {
    public int longestCommonSubsequence(String text1, String text2) {
        int[] dp = new int[text1.length()];
        int count = 0;

        for(char ch : text2.toCharArray()){
            int currL = 0;
            for(int i=0; i<dp.length; i++){
                if(currL < dp[i]){
                    currL = dp[i];
                }else if(ch == text1.charAt(i)){
                    dp[i] = currL+1;
                    count=Math.max(count,currL+1);
                }
            }
        }
        return count;
    }
}